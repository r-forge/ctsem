@article{helgason2004antidepressants,
  title = {Antidepressants and public health in Iceland},
  volume = {184},
  copyright = {{\textcopyright} 2004 Royal College of Psychiatrists},
  issn = {0007-1250, 1472-1465},
  url = {http://bjp.rcpsych.org/content/184/2/157},
  doi = {10.1192/bjp.184.2.157},
  abstract = {Background Major depressive disorder is the second leading cause of disability-adjusted life-years in developed regions of the world and antidepressants are the third-ranking therapy class worldwide.
Aims To test the public health impact of the escalating sales of antidepressants.
Method Nationwide data from Iceland are used as an example to study the effect of sales of antidepressants on suicide, disability, hospital admissions and out-patient visits.
Results Sales of antidepressants increased from 8.4 daily defined doses per 1000 inhabitants per day in 1975 to 72.7 in 2000, which is a user prevalence of 8.7\% for the adult population. Suicide rates fluctuated during 1950{\textendash}2000 but did not show any definite trend. Rates for out-patient visits increased slightly over the period 1989{\textendash}2000 and admission rates increased even more. The prevalence of disability due to depressive and anxiety disorders has notdecreased over the past 25 years.
Conclusions The dramatic increase in the sales of antidepressants has not had any marked impact on the selected public health measures. Obviously, better treatment for depressive disorders is still needed in order to reduce the burden caused by them.},
  language = {en},
  timestamp = {2015-08-29 23:20:01},
  number = {2},
  urldate = {2015-08-10},
  journal = {The British Journal of Psychiatry},
  author = {Helgason, T{\'o}mas and Tomasson, Helgi and Zoega, T{\'o}mas},
  month = feb,
  year = {2004},
  pages = {157--162},
  file = {Snapshot:C\:\\Users\\Charlie\\Dropbox\\MPIB\\zotero\\profile\\storage\\G4F78ATB\\157.html:text/html},
  pmid = {14754829}
}

