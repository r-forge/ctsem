@article{demaris2014combating,
	title = {Combating unmeasured confounding in cross-sectional studies: {Evaluating} instrumental-variable and {Heckman} selection models},
	volume = {19},
	copyright = {(c) 2014 APA, all rights reserved},
	issn = {1939-1463(Electronic);1082-989X(Print)},
	shorttitle = {Combating unmeasured confounding in cross-sectional studies},
	doi = {10.1037/a0037416},
	abstract = {Unmeasured confounding is the principal threat to unbiased estimation of treatment {\textquotedblleft}effects{\textquotedblright} (i.e., regression parameters for binary regressors) in nonexperimental research. It refers to unmeasured characteristics of individuals that lead them both to be in a particular {\textquotedblleft}treatment{\textquotedblright} category and to register higher or lower values than others on a response variable. In this article, I introduce readers to 2 econometric techniques designed to control the problem, with a particular emphasis on the Heckman selection model (HSM). Both techniques can be used with only cross-sectional data. Using a Monte Carlo experiment, I compare the performance of instrumental-variable regression (IVR) and HSM to that of ordinary least squares (OLS) under conditions with treatment and unmeasured confounding both present and absent. I find HSM generally to outperform IVR with respect to mean-square-error of treatment estimates, as well as power for detecting either a treatment effect or unobserved confounding. However, both HSM and IVR require a large sample to be fully effective. The use of HSM and IVR in tandem with OLS to untangle unobserved confounding bias in cross-sectional data is further demonstrated with an empirical application. Using data from the 2006{\textendash}2010 General Social Survey (National Opinion Research Center, 2014), I examine the association between being married and subjective well-being.},
	number = {3},
	journal = {Psychological Methods},
	author = {DeMaris, Alfred},
	year = {2014},
	note = {00000},
	keywords = {*Causal Analysis, *Models, *Statistical Analysis, Causality, Inference},
	pages = {380--397},
	file = {DeMaris_2014_Combating unmeasured confounding in cross-sectional studies.pdf:C\:\\Users\\driver\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ykcu9750.default\\zotero\\storage\\P5CBAIHI\\DeMaris_2014_Combating unmeasured confounding in cross-sectional studies.pdf:application/pdf}
}

