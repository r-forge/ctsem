@article{hamaker2003armabased,
  title = {{ARMA}-Based {SEM} When the Number of Time Points T Exceeds the Number of Cases N: Raw Data Maximum Likelihood},
  volume = {10},
  issn = {1070-5511},
  shorttitle = {{ARMA}-Based {SEM} When the Number of Time Points T Exceeds the Number of Cases N},
  url = {http://dx.doi.org/10.1207/S15328007SEM1003_2},
  doi = {10.1207/S15328007SEM1003_2},
  abstract = {Longitudinal structural equation modeling has generally addressed the time-dependent covariance structure of a relatively small number of repeated measures, T, observed in a relatively large representative sample, N. In contrast, the literature on autoregressive moving average modeling is usually directed at a single realization comprising many observations, that is, N = 1, and T \ensuremath{>} 50. This article deals with autoregressive moving average-based structural equation modeling of time series data, in the situation that N is small, T is intermediate, and T \ensuremath{>} N. The aims of this article are to (a) give a brief overview of the development of alternative formulations of the likelihood function to obtain estimates of autoregressive moving average parameters, in particular the formulation that lies at the basis of M{\'e}lard's algorithm; (b) show the equivalence between the likelihood function to obtain estimates for these parameters, and the raw data likelihood method that can be used in structural equation modeling programs like Mx, and demonstrate this equivalence through simulation experiments; and (c) provide illustrations of this use of Mx with real data.},
  timestamp = {2015-08-29 23:20:01},
  number = {3},
  urldate = {2014-08-04},
  journal = {Structural Equation Modeling: A Multidisciplinary Journal},
  author = {Hamaker, Ellen L. and Dolan, Conor V. and Molenaar, Peter C. M.},
  month = jul,
  year = {2003},
  pages = {352--379},
  file = {Full Text PDF:C\:\\Users\\Charlie\\Dropbox\\MPIB\\zotero\\profile\\storage\\KS3KDTAZ\\Hamaker et al. - 2003 - ARMA-Based SEM When the Number of Time Points T Ex.pdf:application/pdf;Hamaker et al_2003_ARMA-Based SEM When the Number of Time Points T Exceeds the Number of Cases N.pdf:C\:\\Users\\Charlie\\Dropbox\\MPIB\\zotero\\profile\\storage\\22K64C62\\Hamaker et al_2003_ARMA-Based SEM When the Number of Time Points T Exceeds the Number of Cases N.pdf:application/pdf;Snapshot:C\:\\Users\\Charlie\\Dropbox\\MPIB\\zotero\\profile\\storage\\W7GMT6M2\\S15328007SEM1003_2.html:text/html;Snapshot:C\:\\Users\\Charlie\\Dropbox\\MPIB\\zotero\\profile\\storage\\8D979STW\\S15328007SEM1003_2.html:text/html}
}

@article{keijsers2011bidirectional,
  title = {Bidirectional Links and Concurrent Development of Parent-Child Relationships and Boys' Offending Behavior},
  volume = {120},
  issn = {1939-1846(Electronic);0021-843X(Print)},
  doi = {10.1037/a0024588},
  abstract = {This study examined different types of longitudinal associations (i.e., directional links and overlapping developmental changes) between children's delinquency and the quality of parent{\textendash}child relationships from middle childhood to late adolescence. We used 10-wave interview data of 503 boys, their primary caregivers, and their teachers. Our first aim was to unravel the direction of effects between parent{\textendash}child relationships and children's offending. Cross-lagged panel models revealed bidirectional links over time between poorer quality parent{\textendash}child relationships and boys' offending across late childhood (age 7{\textendash}10), early adolescence (age 10{\textendash}13) and middle adolescence (age 13{\textendash}16). Second, we examined the associations between mean changes in delinquency, on the one hand, and mean changes in relationship quality, on the other hand. Although parent{\textendash}child relationships improved during childhood, their quality decreased in early adolescence and remained stable in middle adolescence. Delinquency increased only in middle adolescence. In five out of six models, the slope factors of relationship quality and offending were strongly correlated, indicating that stronger increases in delinquency were associated with stronger decreases in parent{\textendash}child relationship quality across childhood, early adolescence, and middle adolescence. The discussion focuses on the theoretical implications of these two types of longitudinal associations.},
  timestamp = {2015-08-29 23:20:01},
  number = {4},
  journal = {Journal of Abnormal Psychology},
  author = {Keijsers, Loes and Loeber, Rolf and Branje, Susan and Meeus, Wim},
  year = {2011},
  keywords = {*Adolescent Development,*Childhood Development,*Juvenile Delinquency,*Parent Child Relations,Parents,Relationship Quality},
  pages = {878--889},
  file = {Keijsers et al_2011_Bidirectional links and concurrent development of parent-child relationships.pdf:C\:\\Users\\Charlie\\Dropbox\\MPIB\\zotero\\profile\\storage\\XWJ8RVDD\\Keijsers et al_2011_Bidirectional links and concurrent development of parent-child relationships.pdf:application/pdf}
}

@article{savalei2008constrained,
  title = {Constrained Versus Unconstrained Estimation in Structural Equation Modeling},
  volume = {13},
  issn = {1939-1463(Electronic);1082-989X(Print)},
  doi = {10.1037/1082-989X.13.2.150},
  abstract = {Recently, R. D. Stoel, F. G. Garre, C. Dolan, and G. van den Wittenboer (2006) reviewed approaches for obtaining reference mixture distributions for difference tests when a parameter is on the boundary. The authors of the present study argue that this methodology is incomplete without a discussion of when the mixtures are needed and show that they only become relevant when constrained difference tests are conducted. Because constrained difference tests can hide important model misspecification, a reliable way to assess global model fit under constrained estimation would be needed. Examination of the options for assessing model fit under constrained estimation reveals that no perfect solutions exist, although the conditional approach of releasing a degree of freedom for each active constraint appears to be the most methodologically sound one. The authors discuss pros and cons of constrained and unconstrained estimation and their implementation in 5 popular structural equation modeling packages and argue that unconstrained estimation is a simpler method that is also more informative about sources of misfit. In practice, researchers will have trouble conducting constrained difference tests appropriately, as this requires a commitment to ignore Heywood cases. Consequently, mixture distributions for difference tests are rarely appropriate.},
  timestamp = {2015-08-29 23:20:01},
  number = {2},
  journal = {Psychological Methods},
  author = {Savalei, Victoria and Kolenikov, Stanislav},
  year = {2008},
  keywords = {*Chi Square Test,*Estimation,Mathematical Modeling},
  pages = {150--170}
}

@article{singer1998continuous,
