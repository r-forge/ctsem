	month = may,
	year = {2013},
	note = {00020 PMID: 23661762},
	pages = {756--759},
	file = {Freund et al_2013_Emergence of Individuality in Genetically Identical Mice.pdf:C\:\\Users\\driver\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ykcu9750.default\\zotero\\storage\\FCTUUG2D\\Freund et al_2013_Emergence of Individuality in Genetically Identical Mice.pdf:application/pdf;Snapshot:C\:\\Users\\driver\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\ykcu9750.default\\zotero\\storage\\DXMDI233\\756.html:text/html}
