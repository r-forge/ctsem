@article{kampfer2013sunny,
	title = {On the {Sunny} {Side} of {Life}: {Sunshine} {Effects} on {Life} {Satisfaction}},
	volume = {110},
	issn = {0303-8300, 1573-0921},
	shorttitle = {On the {Sunny} {Side} of {Life}},
	url = {http://link.springer.com/article/10.1007/s11205-011-9945-z},
	doi = {10.1007/s11205-011-9945-z},
	abstract = {This paper seeks to analyze the influence of the weather on a person{\textquoteright}s self-reported life satisfaction. On a theoretical level, it is claimed that {\textquoteleft}nice{\textquoteright} weather can improve the affective well-being of a person. Given this, it is argued that affects can, in turn, have an impact on that person{\textquoteright}s general assessment of his or her life. In particular, it is expected that people would report a higher life satisfaction on days with unambiguously {\textquoteleft}nice{\textquoteright} weather. Data from three German large-scale surveys are used to test empirically to what extent self-reported life satisfaction is determined by the weather. All in all, the results are mostly consistent with the initial hypothesis. In all three samples those respondents surveyed on days with exceptionally sunny weather reported a higher life satisfaction compared to respondents interviewed on days with {\textquoteleft}ordinary{\textquoteright} weather. In two out of three samples, this difference was statistically significant. Hence, the supposed sunshine effect on peoples{\textquoteright} life satisfaction does indeed exist. Implications of these findings are discussed in a conclusion.},
	language = {en},
	number = {2},
	urldate = {2013-12-19},
	journal = {Social Indicators Research},
	author = {K{\"a}mpfer, Sylvia and Mutz, Michael},
	month = jan,
