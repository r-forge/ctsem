@article{brown2008well-being,
	title = {Well-{Being}, {Sleep}, {Exercise} {Patterns}, and the {Menstrual} {Cycle}: {A} {Comparison} of {Natural} {Hormones}, {Oral} {Contraceptives} and {Depo}-{Provera}},
